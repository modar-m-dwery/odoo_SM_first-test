<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="level_action" model="ir.actions.act_window">
        <field name="name">Level</field>
        <field name="res_model">school_management.level</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="sm_level_view_tree" model="ir.ui.view">
        <field name="name">Level</field>
        <field name="model">school_management.level</field>
        <field name="arch" type="xml">
            <tree string="Level">
                <field name="level"></field>
            </tree>
        </field>
    </record>

    <record id="sm_level_view_form" model="ir.ui.view">
        <field name="name">school_management.level.view.form</field>
        <field name="model">school_management.level</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="level"></field>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>